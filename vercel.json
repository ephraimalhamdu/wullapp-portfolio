{
  "version": 2,
  "builds": [
    { "src": "vite.config.js", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/contact.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/contact", "dest": "/api/contact.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
